{
	"name": "shineunited/composer-build-scss",
	"type": "composer-plugin",
	"license": "MIT",
	"require": {
		"php": ">=5.6",
		"composer-plugin-api": "^1.0 || ^2.0",
		"shineunited/composer-build": "^0.1.2",
		"scssphp/scssphp": "^1.4"
	},
	"require-dev": {
		"composer/composer": ">=1.0",
		"phpunit/phpunit": ">=5.5.0"
	},
	"autoload": {
		"psr-4": {
			"ShineUnited\\ComposerBuildPlugin\\Scss\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ShineUnited\\ComposerBuildPlugin\\Scss\\Tests\\": "tests/"
		}
	},
	"extra": {
		"class": "ShineUnited\\ComposerBuildPlugin\\Scss\\Plugin"
	}
}
